{
    "name":  "example",
    "labels":  {
                   "app":  "example",
                   "tier":  "frontend"
               },
    "ports":  [
                  {
                      "port":  80,
                      "targetPort":  81
                  },
                  {
                      "port":  8081,
                      "protocol":  "TCP",
                      "targetPort":  81
                  }
              ],
    "mode":  "replicated",
    "replicas":  2,
    "container-labels":  {
                             "app":  "example",
                             "tier":  "frontend"
                         },
    "env":  {
                   "sqlserver":  "srv1",
                   "ASPNETCORE_ENVIRONMENT":  "Development"
               },
    "constraints":  ["node.role==worker","node.platform.os==linux"],
    "secrets":  [
                    {
                        "source":  "example-secret1",
                        "target":  "/run/secrets/example-secret1"
                    },
                    {
                        "source":  "example-secret2",
                        "target":  "/run/secrets/example-secret2"
                    }
                ],
    "image":  "nginx"
}